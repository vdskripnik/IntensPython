/*Дано число N<99. Дописать в начало и в конец числа цифру k. (диапазон возможных k, например, от 1 до 5).*/
#include <stdio.h>

int main() {
    int N, k;
    
    printf("Введите число N (N < 99): ");
    scanf("%d", &N);
    
    if (N >= 99) {
        printf("Ошибка: N должно быть меньше 99.\n");
        return 1; // Завершение программы с ошибкой
    }

    printf("Введите цифру k (от 1 до 5): ");
    scanf("%d", &k);
    
    if (k < 1 || k > 5) {
        printf("Ошибка: k должно быть в диапазоне от 1 до 5.\n");
        return 1; // Завершение программы с ошибкой
    }

    // Формирование нового числа
    int new_number = k * 100 + N * 10 + k; // Добавляем k в начало и конец

    printf("Новое число: %d\n", new_number);

    return 0;
}
